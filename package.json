{
  "type": "module",
  "name": "bee-agent-chat",
  "version": "1.0.0",
  "description": "Interactive chat interface powered by Bee Agent Framework with Groq LLaMA 3.1",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run copy-static",
    "copy-static": "node -e \"require('fs').cpSync('public', 'dist/public', {recursive: true})\"",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "tsx src/test.ts",
    "test:prod": "NODE_ENV=production tsx src/test.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "healthcheck": "curl -f http://localhost:$PORT/health || exit 1"
  },
  "keywords": [
    "ai",
    "chatbot",
    "bee-agent",
    "groq",
    "llama",
    "typescript",
    "express",
    "socket.io"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdulAbdullah/bee_agent_framework.git"
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "bee-agent-framework": "^0.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
