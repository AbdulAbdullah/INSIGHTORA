{
  "type": "module",
  "name": "bee-agent-chat",
  "version": "1.0.0",
  "description": "Interactive chat interface powered by Bee Agent Framework with Groq LLaMA 3.1",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run copy-static",
    "copy-static": "node -e \"require('fs').cpSync('public', 'dist/public', {recursive: true})\"",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "tsx src/test.ts",
    "test:prod": "NODE_ENV=production tsx src/test.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "healthcheck": "curl -f http://localhost:$PORT/health || exit 1",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "ai",
    "chatbot",
    "bee-agent",
    "groq",
    "llama",
    "typescript",
    "express",
    "socket.io"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdulAbdullah/bee_agent_framework.git"
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^3.0.2",
    "bee-agent-framework": "^0.1.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prisma": "^6.17.1",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
